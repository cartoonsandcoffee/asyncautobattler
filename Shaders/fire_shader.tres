	vec2 n_out17p0 = n_out20p0 * n_in17p1 + n_out21p0;


	vec4 n_out3p0;
// Texture2D:3
	n_out3p0 = texture(Texture_1, n_out17p0);
	float n_out3p1 = n_out3p0.r;


	vec4 n_out6p0;
// Texture2D:6
	n_out6p0 = texture(Texture_2, UV);
	float n_out6p1 = n_out6p0.r;


// FloatOp:23
