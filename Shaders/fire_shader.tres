	float n_out13p1 = n_out13p0.r;


// FloatOp:25
	float n_out25p0 = n_out24p0 - n_out13p1;


// Clamp:27
	float n_in27p1 = 0.00000;
	float n_in27p2 = 1.00000;
	float n_out27p0 = clamp(n_out25p0, n_in27p1, n_in27p2);


	vec4 n_out16p0;
// Texture2D:16
	n_out16p0 = texture(Texture_Color_1D, vec2(n_out27p0));
	float n_out16p4 = n_out16p0.a;


// Output:0
	COLOR.rgb = vec3(n_out16p0.xyz);
	COLOR.a = n_out16p4;


}
